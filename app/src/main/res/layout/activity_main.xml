<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingLeft="18dp"
    android:paddingRight="18dp"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/bt_bit_operations"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="java位运算" />

    <Button
        android:id="@+id/bt_collection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="java集合基础" />

    <Button
        android:id="@+id/bt_Queue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="java队列特写" />

    <Button
        android:id="@+id/bt_reflection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="java反射" />

    <Button
        android:id="@+id/bt_annotation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Java注解类型(@Annotation)" />
</LinearLayout>